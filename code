import pandas as pd 
import numpy as np 
import matplotlib.pyplot as plt 
import seaborn as sns 

data = pd.Series([11, 20, 22, 31, 32, 33, 41, 42, 43, 44, 51, 52, 53, 54, 55, 61, 62, 63, 64, 65, 66, 71, 72, 73, 74, 75, 76, 77, 81, 82, 83, 84, 85, 86, 87, 88, 91, 92, 93, 94, 95, 96, 97, 98, 99])
print(data.hist(bins=4, alpha=0.5))  # crea un histograma con cuatro contenedores
data.hist( bins=[11, 20, 30, 40, 50, 60, 70, 80, 90, 99], alpha=0.7)